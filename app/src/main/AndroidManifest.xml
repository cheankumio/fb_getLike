<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appers.klc.fblottery" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/lottery_s"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.appers.klc.fblottery.WelcomePage" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.appers.klc.fblottery.MainActivity"/>
        <activity android:name="com.appers.klc.fblottery.PostListView"/>
        <activity android:name="com.appers.klc.fblottery.Lottery_Activity"/>
        <activity android:name="com.appers.klc.fblottery.SetDisplayData_Activity"/>
        <activity android:name="com.appers.klc.fblottery.LuckActivity"/>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>